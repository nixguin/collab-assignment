{
  "version": 2,
  "builds": [
    {
      "src": "clean/api/main.py",
      "use": "@vercel/python"
    },
    {
      "src": "clean/web/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/clean/api/main.py"
    },
    {
      "src": "/",
      "dest": "/clean/web/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/clean/web/$1"
    }
  ]
}
